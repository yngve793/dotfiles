{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (lldb)",
            "type": "lldb",
            "request": "launch",
            "program": "/opt/homebrew/bin/ctest",
            "args": [
                "--test-dir",
                "build",
                "--output-on-failure"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false
        },
        {
            "name": "(ctest) Launch2",
            "type": "cppdbg",
            "cwd": "${workspaceFolder}",
            "request": "launch",
            "program": "/opt/homebrew/bin/ctest",
            "launchCompleteCommand": "exec-run",
            "args": [
                "--test-dir",
                "build",
                "--output-on-failure"
            ],
            "MIMode": "lldb",
            "stopAtEntry": true
        },
        {
            "name": "Launch Package2",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/query/main.go",
            "args": [
                "--storage-accounts=https://vdsbenchmark.blob.core.windows.net"
            ]
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/query/main.go",
            "args": [
                "--storage-accounts=https://vdsbenchmark.blob.core.windows.net:443"
            ]
        },
        {
            "name": "Launch onevdstest",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/query/main.go",
            "args": [
                "--storage-accounts=https://onevdstest.blob.core.windows.net",
                "--cache-size=100"
            ]
        },
        {
